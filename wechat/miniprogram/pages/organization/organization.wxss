.morePage{
  background: linear-gradient(white, #D6EAD4); 
  height: 100vh;
}

.UCenter-bg {
  background: linear-gradient(160deg,rgb(254, 218, 75), #D6EAD4); 
  background-size: cover;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  padding-bottom: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
  box-shadow:0px 4px 13px #81B0B2;
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 160rpx;
  height: 160rpx;
}

